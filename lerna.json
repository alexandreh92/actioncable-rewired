{
  "packages": [
    "packages/*"
  ],
  "npmClient": "yarn",
  "useWorkspaces": true,
  "version": "0.1.8-alpha.11",
  "command": {
    "publish": {
      "registry": "https://registry.npmjs.org"
    },
    "version": {
      "conventionalCommits": true,
      "allowBranch": [
        "main",
        "release/*"
      ],
      "message": "chore(release): publish",
      "noPrivate": true
    }
  }
}
